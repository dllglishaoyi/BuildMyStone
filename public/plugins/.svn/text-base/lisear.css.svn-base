.lisear-container *{
    margin: 0;
    padding: 0;
    
}
.lisear-container{
    color: #212121;
    height: 32px;
    line-height: 32px;
    padding: 10px 0px;
    display: block;
    border-bottom: 1px solid #dbdbdb;
    background: #ffffff;
    position: relative;
}
.lisear-input-area{
    /*display: -webkit-box;*/
    display: block;
    padding: 0 6px;
    height: 32px;
    position: relative;
    /*background: #F1F5F8;*/
}
.lisear-input-area input{
    padding-left: 30px;
    border: none;
    background: none;
    background: #F1F5F8;
    width: 100%;
    height: 30px;
    font-size: 14px;
    -webkit-border-radius: 16px;
}
.lisear-input-area .lisear-search-btn{
    width: 18px;
    height: 18px;
    display: block;
    position: absolute;
    background-image: url(search.png);
    /*background-repeat: none;*/
    background-size:18px 18px;
    background-repeat:no-repeat;
    left: 13px;
    top: 8px;
}

.lisear-input-area .lisear-clear-btn{
    width: 18px;
    height: 18px;
    display: none;
    position: absolute;
    background-image: url(close.png);
    /*background-repeat: none;*/
    background-size:18px 18px;
    background-repeat:no-repeat;
    right: 13px;
    top: 8px;
}
.lisear-list{
    position: relative;
    padding: 0;
    list-style: none;
    color: #666;
    font-size: 14px;
    /*display: -webkit-box;*/
    width: 100%;
    /*overflow: hidden;*/
    z-index: 1000;
}

.lisear-list a{
    text-decoration:none;
    display: block;
    display: block;
    background: #ffffff;
}

.lisear-list li{
    overflow: hidden;
    display: block;
    background: #ffffff;
    color: #666;
    height: 43px;
    line-height: 43px;
    /*background: #f9f9f9;*/
    border-bottom: 1px solid #dedede;
    padding: 0 16px;
}

